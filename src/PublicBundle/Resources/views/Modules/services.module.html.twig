<!-- section begin -->
      <section id="section-services" class="bg-grey">
          <div class="container">
              <div class="row">
                  <div class="text-center">
                      <h2 class="wow fadeInUp" data-wow-delay="0">{% trans %} _services_title{% endtrans %}
                          <span>{% trans %} _services_subtitle{% endtrans %}</span>
                      </h2>
                      <div class="small-border wow flipInY" data-wow-delay=".2s" data-wow-duration=".8s"></div>
                  </div>

                  <div class="divider-single"></div>

                  <div class="col-md-4 wow fadeInUp" data-wow-delay="0">
                      <div class="box-with-icon-left">
                          <i class="fa fa-send-o icon-big"></i>
                          <div class="text">
                              <h2>{% trans %} _services_left_title{% endtrans %}</h2>
                              <p>{% trans %} _services_left_parragraph{% endtrans %}</p>
                              <div class="divider-single"></div>
                              <a href="service-details-1.html" class="btn-text">{% trans %} _services_left_button{% endtrans %}</a>
                          </div>
                      </div>
                  </div>

                  <div class="col-md-4 wow fadeInUp" data-wow-delay="0">
                      <div class="box-with-icon-left">
                          <i class="fa fa-puzzle-piece icon-big"></i>
                          <div class="text">
                            <h2>{% trans %} _services_center_title{% endtrans %}</h2>
                            <p>{% trans %} _services_center_parragraph{% endtrans %}</p>
                              <div class="divider-single"></div>
                              <a href="service-details-2.html" class="btn-text">{% trans %} _services_center_button{% endtrans %}</a>
                          </div>
                      </div>
                  </div>

                  <div class="col-md-4 wow fadeInUp" data-wow-delay="0">
                      <div class="box-with-icon-left">
                          <i class="fa fa-truck icon-big"></i>
                          <div class="text">
                            <h2>{% trans %} _services_right_title{% endtrans %}</h2>
                            <p>{% trans %} _services_right_parragraph{% endtrans %}</p>
                            <div class="divider-single"></div>
                              <a href="service-details-3.html" class="btn-text">{% trans %} _services_right_button{% endtrans %}</a>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </section>
<!-- section close -->

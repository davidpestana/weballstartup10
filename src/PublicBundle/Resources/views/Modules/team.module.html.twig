<div class="col-md-12">
    <div class="divider-line"></div>

    <div class="text-center">
        <h2 class="wow fadeInUp" data-wow-delay="0">{% trans %}_team_title{% endtrans %}</h2>
        <div class="small-border wow flipInY" data-wow-delay=".2s" data-wow-duration=".8s"></div>
    </div>

    <div class="col-md-3">
